<%= form_with(model: @order) do |f| %>
  <div>
    <%= f.label :warehouse_id %><br>
    <%= f.collection_select :warehouse_id, @warehouses, :id, :full_description %>
  </div>

  <div>
    <%= f.label :supplier_id %><br>
    <%= f.collection_select :supplier_id, @suppliers, :id, :corporate_name %>
  </div>

  <div>
    <%= f.label :estimated_delivery_date %><br>
    <%= f.date_field :estimated_delivery_date, class: 'form-control' %>
  </div>

  <div>
    <%= f.submit 'Gravar', class: 'btn btn-success' %>
  </div>
<% end %>